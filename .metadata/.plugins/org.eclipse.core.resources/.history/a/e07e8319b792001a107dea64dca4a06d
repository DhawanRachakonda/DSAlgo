package dsalgo.bst;

public class TreeClient {

	private static Tree tree;

	public static void main(String args[]) {

		int[] nums = new int[] { 4, 5, 6, 11, 7, 2, 8, 3, 9, 1 };

		tree = new Tree();

		for(int i=0; i<nums.length; i++) {
			tree.addValue(nums[i]);
		}
		
		System.out.println(tree.toString());

	}

}
